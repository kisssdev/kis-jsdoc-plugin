{
  "name": "kis-jsdoc-plugin",
  "version": "1.4.4",
  "description": "A jsdoc plugin associated with a custom template that generates markdown documentation in a 'Keep It Simple' way.",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./index.js ./templates/**/*.js",
    "test": "./node_modules/.bin/jasmine",
    "doc": "./node_modules/.bin/jsdoc -c ./.jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kisssdev/kis-jsdoc-plugin.git"
  },
  "keywords": [
    "jsdoc",
    "plugin",
    "markdown"
  ],
  "author": "KisssDev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kisssdev/kis-jsdoc-plugin/issues"
  },
  "homepage": "https://github.com/kisssdev/kis-jsdoc-plugin#readme",
  "dependencies": {
    "fs-extra": "^5.0.0",
    "handlebars": "^4.7.6",
    "line-column": "^1.0.2",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jsdoc": "^3.6.4",
    "jsdoc-escape-at": "^1.0.1",
    "kis-jsdoc-plugin": "^1.1.0",
    "mock-require": "^3.0.1"
  },
  "files": [
    "index.js",
    "templates",
    "LICENSE"
  ]
}
