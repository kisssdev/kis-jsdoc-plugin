{
  "extends": "airbnb-base",
  "rules": {
    "max-len": ["error", {
      "ignoreComments": true,
      "code": 120
    }],
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "ignore"
    }]
  }
}
